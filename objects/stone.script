local speed = 150

function update(self, dt)
	local p = go.get_position()
	p.y = p.y + speed * dt
	if p.y > 1200 then
		go.delete()
	end
	go.set_position(p)
end

function on_message(self, message_id, message, sender)
	if message_id == hash("collision_response") and message.other_id == hash("/shovel") then
		print("test")
		msg.post("main#gui", "gameover")
		go.delete()
	end
end